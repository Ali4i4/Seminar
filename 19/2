#include <iostream>

using namespace std;

int main()
{
    int a;
    cout << "Введите a" << endl;
    cin >> a;
    while (a > 10) // до тех пор пока а больше 10 отсекаем по разрядам, пока число не станет от 0 д 10, что и будет ответом //
    {
        a = a / 10; 
        cout << a << endl;
    }

    return 0;
}
